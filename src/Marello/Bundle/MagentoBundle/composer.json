{
    "name": "marello/magento-bundle",
    "type": "symfony-bundle",
    "description": "Marello Magento Bundle",
    "keywords": ["Marello", "Magento", "Product"],
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "2.8.*, !=2.8.10",
        "doctrine/orm": ">=2.3,<2.4-dev",
        "oro/log": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/business-entities-bundle": "dev-master",
        "marello/integration-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "target-dir": "Marello/Bundle/MagentoBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
