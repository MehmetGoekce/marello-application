{
    "name": "marellocommerce/marello-enterprise",
    "description": "Marello Enterprise ERP for Commerce",
    "homepage": "https://github.com/marellocommerce/marello-enterprise.git",
    "license": "Commercial",
    "authors": [
        {
            "name": "Madia B.V.",
            "homepage": "http://www.marello.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "MarelloEnterprise\\Bundle": "src/"
         }
    },
    "repositories": [
       {
            "type": "composer",
            "url": "http://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "oro/platform-enterprise": "2.2.*",
        "marellocommerce/marello": "1.2.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}